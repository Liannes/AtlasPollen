  "license": "MIT",
  "scripts": {
    "test": "jest",
    "start": "webpack serve",
    "watch": "webpack --watch",
    "build": "webpack",
    "build-prod": "webpack --mode=production",
    "build-details": "webpack --mode=production --stats-error-details",
    "prettify": "npx prettier \"**/*.*(js|jsx)\" --ignore-path=.prettierignore --write",
    "lint": "eslint src --ext .js,.jsx",
    "lint-fix": "eslint src --ext .js,.jsx --fix",
    "stats": "webpack --mode=production --json > build/stats.json && webpack-bundle-analyzer build/stats.json"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^Assets(.*)$": "<rootDir>/src/assets$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/fileMockup.js",
